{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate app",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "lint": "eslint src/**/*.ts src/**/*.tsx && stylelint 'src/**/*.scss' --syntax scss",
    "lintfix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "serve": "webpack-dev-server --mode development --hot --progress",
    "start": "npm run serve",
    "test:watch": "jest --watchAll",
    "test": "jest",
    "watch": "webpack --mode development --progress --watch",
    "backend:mock": "json-server --routes mockend/routes.json --watch mockend/mockDb.json",
    "start:mock": "concurrently -k -p \"[{name}]\" -n \"HTTP-Server,Mock-Backend\" -c \"cyan.bold,green.bold\" \"npm run serve\" \"npm run backend:mock\""
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "@babel/preset-react": "7.0.0",
    "@material-ui/types": "^4.1.1",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/lodash": "4.14.135",
    "@types/node": "12.0.12",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-select": "^3.0.0",
    "@types/styled-components": "^4.1.16",
    "@types/webpack": "4.4.34",
    "@types/webpack-dev-server": "3.1.6",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "concurrently": "4.1.1",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "json-server": "0.15.0",
    "source-map-loader": "0.2.4",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.2",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-stylish": "^0.1.8"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "axios": "0.19.0",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-select": "^3.0.4",
    "styled-components": "^4.3.2"
  }
}
